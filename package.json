{
    "name": "vscode-react-create",
    "displayName": "vscode-react-create",
    "description": "react create component",
    "version": "0.0.1",
    "publisher": "hengkx",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.createFolder",
                "title": "Create React Folder",
                "category": "Files and Folder"
            }
        ],
        "keybindings": [
            {
                "command": "extension.createFolder",
                "key": "ctrl+alt+n"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.createFolder",
                    "group": "navigation@-createFileFolder"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}